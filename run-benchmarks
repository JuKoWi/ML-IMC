julia mc-serial.jl LJ-init-1E5.in > serial-1E5.out
julia mc-serial.jl LJ-init-1E6.in > serial-1E6.out
julia mc-serial.jl LJ-init-1E7.in > serial-1E7.out

julia -p 1 mc-parallel.jl LJ-init-1E5.in > p1-1E5.out
julia -p 1 mc-parallel.jl LJ-init-1E6.in > p1-1E6.out
julia -p 1 mc-parallel.jl LJ-init-1E7.in > p1-1E7.out

julia -p 2 mc-parallel.jl LJ-init-1E5.in > p2-1E5.out
julia -p 2 mc-parallel.jl LJ-init-1E6.in > p2-1E6.out
julia -p 2 mc-parallel.jl LJ-init-1E7.in > p2-1E7.out

julia -p 4 mc-parallel.jl LJ-init-1E5.in > p4-1E5.out
julia -p 4 mc-parallel.jl LJ-init-1E6.in > p4-1E6.out
julia -p 4 mc-parallel.jl LJ-init-1E7.in > p4-1E7.out

julia -p 6 mc-parallel.jl LJ-init-1E5.in > p6-1E5.out
julia -p 6 mc-parallel.jl LJ-init-1E6.in > p6-1E6.out
julia -p 6 mc-parallel.jl LJ-init-1E7.in > p6-1E7.out

